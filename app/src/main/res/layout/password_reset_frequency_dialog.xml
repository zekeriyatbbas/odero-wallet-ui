<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/MainFrame"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/SelectionFrame"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/frame_background"
        android:minWidth="390dp"
        android:minHeight="295dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/frequency01"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:clickable="true"
            android:fontFamily="@font/dm_sans_medium"
            android:gravity="center"
            android:lineHeight="24dp"
            android:text="1 ay"
            android:textAlignment="center"
            android:textColor="@color/odero_black_gray"
            android:textFontWeight="500"
            android:textSize="22dp" />

        <LinearLayout
            android:id="@+id/spacing1"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="50dp"
            android:paddingRight="50dp">

            <FrameLayout
                android:id="@+id/divider1"
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:background="@drawable/divider">

            </FrameLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/frequency03"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:clickable="true"
            android:fontFamily="@font/dm_sans_medium"
            android:gravity="center"
            android:lineHeight="24dp"
            android:text="3 ay"
            android:textAlignment="center"
            android:textColor="@color/odero_black_gray"
            android:textFontWeight="500"
            android:textSize="22dp" />

        <LinearLayout
            android:id="@+id/spacing2"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingLeft="50dp"
            android:paddingRight="50dp">

            <FrameLayout
                android:id="@+id/divider2"
                android:layout_width="match_parent"
                android:layout_height="1.5dp"
                android:background="@drawable/divider">

            </FrameLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/frequency06"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:clickable="true"
            android:fontFamily="@font/dm_sans_medium"
            android:gravity="center"
            android:lineHeight="24dp"
            android:text="6 ay"
            android:textAlignment="center"
            android:textColor="@color/odero_black_gray"
            android:textFontWeight="500"
            android:textSize="22dp" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>